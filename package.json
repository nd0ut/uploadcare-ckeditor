    "archiver": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-config-uploadcare": "^1.1.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-cp": "^0.1.3",
